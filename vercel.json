{
    "version": 2,
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/(?!.*\\.(js|css|png|jpg|jpeg|svg|ico|json|webp|avif|gif|woff2?|eot|ttf|otf)$)(.*)",
            "destination": "/index.html"
        }
    ]
}